
#include "doc/graphs/header.dot"

digraph graphname {
    GRAPH_FORMAT;

    Init    [];
    Error   [];
    Session [shape="rect"];
    Device  [shape="rect"];
    Stream  [shape="rect"];

    Init -> Session   [label = "begin"];

    Session -> Device [label = "select"];
    Device -> Init    [label = "end"];
    Device -> Stream  [label = "start"];
    Stream -> Device  [label = "stop"];

    Init -> Error;
    // Stream -> Error [style="invisible", arrowhead="none"];
    Device -> Error;      
}
