[package "dummy"]

# Mac OS X dependencies

[package "portaudio-osx"]
    site    = doremir
    version = 19.0.0
    subpath = external/portaudio/result
    path    = include, lib
    mode    = get/put

[package "portmidi-osx"]
    site    = doremir
    version = 2.17.0
    subpath = external/portmidi/result
    path    = include, lib
    mode    = get/put

[package "sndfile-osx"]
    site    = doremir
    version = 1.0.25
    subpath = external/sndfile/result
    path    = bin, include, lib, share
    mode    = get/put

[package "fluidsynth-osx"]
    site    = doremir
    version = 1.1.5
    subpath = external/fluidsynth/result
    path    = bin, include, lib, share
    mode    = get/put

[package "libogg-osx"]
    site    = doremir
    version = 1.3.1
    subpath = external/libogg/result
    path    = include, lib, share
    mode    = get/put

[package "libvorbis-osx"]
    site    = doremir
    version = 1.3.2
    subpath = external/libvorbis/result
    path    = include, lib, share
    mode    = get/put


