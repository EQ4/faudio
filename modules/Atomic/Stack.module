
/**
    Provides an atomic queue (first in, last out) data structure
    @defgroup DoremirAtomicStack Concurrency: Atomic stacks
 */
module Doremir.Atomic.Stack
{
    type Stack = Intptr;
    
    create : () -> Stack;
    destroy : (Stack) -> Void;
    
    read : (Stack) -> Intptr;
    write : (Stack, Intptr) -> Intptr;
}

