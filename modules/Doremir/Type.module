
/**
    @addtogroup DoremirType
    
    Provides signal types.

    Name                     | Description
    ------------------------ | ----------------------------
    `int8`                   | 8-bit integer
    `int16`                  | 16-bit integer
    `int32`                  | 32-bit integer
    `int64`                  | 64-bit integer
    `float`                  | 32-bit floating point
    `double`                 | 64-bit floating point
    `(A, B)`                 | pair of *A* and *B*
    `[A x n]`                | vector of A
    `A -> B`                 | function from *A* to *B*
    `~A`                     | signal of *A*
    `A ~> B`                 | processor from *A* to *B*

    
 */
module Doremir.Type
{
    import Doremir;

    type Type = opaque;
}