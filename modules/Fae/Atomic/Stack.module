
/** 
    @addtogroup FaeAtomicStack
    
    Mutable atomic (lock-free) stacks.

    @see 
    - [Data structures](@ref DataStructures)

 */
module Fae.Atomic.Stack
{
    import Fae;

    type Stack = opaque;
    
    create : () -> Stack;
    destroy : Stack -> Void;
    
    read : Stack -> Ptr;
    write : (Stack, Ptr) -> Bool;
}

