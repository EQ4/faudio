
/** 
    @addtogroup Doremir
    
    Core definitions. Import \ref DoremirAudioEngine instead of this.
    
 */
module Doremir
{
    import Doremir.Std;

    type Ptr = Void*;
    type Nullary = (() -> Ptr)*;
    type Unary   = (Ptr -> Ptr)*;
    type Binary  = ((Ptr, Ptr) -> Ptr)*;
    type Closure = struct { 
        function : (Intptr -> Intptr)*, 
        value    : Intptr };    
}